{
    "Role": {
        "Path": "/",
        "RoleName": "bar-lambda-converter-ENV",
        "AssumeRolePolicyDocument": {
            "Version": "2012-10-17",
            "Statement": [
                {
                    "Effect": "Allow",
                    "Principal": {
