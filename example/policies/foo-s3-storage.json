{
    "Policy": {
        "PolicyName": "foo-s3-storage-ENV",
        "Path": "/"
    },
    "Document": {
        "Version": "2012-10-17",
        "Statement": [
            {
                "Effect": "Allow",
                "Action": [
                    "s3:PutObject",
                    "s3:GetObject",
                    "s3:DeleteObject"
                ],
                "Resource": "arn:aws:s3:::foo-s3-storage-ENV-ACCOUNT_ID/*"
            }
        ]
    }
}